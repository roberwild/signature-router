{
  "name": "dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000 --turbo",
    "build": "pnpm run db:migrate:prod && pnpm run db:seed:prod && next build",
    "build:local": "next build",
    "db:migrate": "cd ../../packages/database && pnpm run migrate",
    "db:migrate:prod": "cd ../../packages/database && pnpm run migrate:prod",
    "db:seed:prod": "cd ../../packages/database && pnpm run seed:prod",
    "start": "next start --port 3000",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "clean": "git clean -xdf .cache .next .turbo build dist node_modules",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,mdx}\"",
    "format:fix": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,mdx}\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:check": "tsx scripts/check-untranslated.ts",
    "i18n:test": "tsx scripts/test-locale-switching.ts",
    "i18n:perf": "tsx scripts/measure-i18n-performance.ts"
  },
